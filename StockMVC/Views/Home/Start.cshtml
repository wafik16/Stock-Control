

@{
    ViewData["Title"] = "Log in";
    Layout = "~/Views/Shared/_Login.cshtml";
}

<h2>@ViewData["Title"]</h2>
<div class="row">
    <div class="col-md-4">
        <section>

            
        </section>
    </div>
    <div class="col-md-6 col-md-offset-2">
        <section>
            <a class="btn btn-primary" style="width:auto"
               asp-controller="Identity" asp-action="/Account/Login">
                LOGIN
            </a>

            <a class="btn btn-primary" style="width:auto"
               asp-controller="Identity" asp-action="/Account/Register">
               REGISTER
            </a>
        </section>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
